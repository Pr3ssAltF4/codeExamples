{#==========================                                                       ===============================================#}
{#==========================                          Agenda                       ===============================================#}
{#==========================                                                       ===============================================#}
{#================================================================================================================================#}


            <div class="row">
                <div class="col-md-6">
                  <div class="box" style="margin-left: 15%;">
                    <div class="box-header with-border">
                      <h3 class="box-title">Your Agenda</h3>
                      <div class="box-tools pull-right">
                        <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        <button class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                      </div>
                    </div><!-- /.box-header -->
                    <div class="box-body">
                      <div class="row">
                        <div class="col-md-12">
                            <!-- Display Appointment Modal -->
                          <div class="modal fade" id="displayEventModal" role="dialog">
                            <div class="modal-dialog">

                              <!-- Modal content-->
                              <div class="modal-content">
                                <div class="modal-header">
                                  <button type="button" class="close" data-dismiss="modal">&times;</button>
                                  <h4 id="modalTitle" class="modal-title"></h4>
                                </div>
                                <div id="modalBody" class="modal-body">
                                    <label>Patient</label>
                                    <div id="modalPatient"></div>
                                    <div id="modalID"></div>
                                    <label>Description</label>
                                    <div id="modalDescription"></div>
                                    <label>Location</label>
                                    <div id="modalLocation"></div>
                                    <label>Date</label>
                                    <div id="modalDate"></div>
                                    <br>
                                    <button style="margin-left:10px;float:right;" type="button" class="btn btn-warning btn-md" id="update" data-dismiss="modal">Edit</button>
                                    <button style="float:right;" type="button" class="btn btn-danger btn-md" id="delete" data-dismiss="modal">Delete</button>
                                </div>
                                <div class="modal-footer">
                                </div>
                              </div>
                            </div>
                          </div>
                            <!-- Create Appointment Modal -->
                            <div class="modal fade" id="createEventModal" role="dialog">
                            <div class="modal-dialog">
                              <!-- Modal content-->
                              <div class="modal-content">
                                <div class="modal-header">
                                  <button type="button" class="close" data-dismiss="modal">&times;</button>
                                  <h4 class="modal-title">New Appointment</h4>
                                </div>
                                <div class="modal-body">
                                  <form method="post" action=".">{% csrf_token %}
                                    {% for box in create_form %}
                                    <div class="form-group">
                                        {% if box.name == "title"%}
                                            <label>Title:</label>
                                        {% elif box.name == "description"%}
                                            <label>Description:</label>
                                        {% elif box.name == "date"%}
                                            <label>Date:</label>
                                        {% elif box.name == "start_time"%}
                                            <label>Start Time:</label>
                                        {% elif box.name == "end_time"%}
                                            <label>End Time:</label>
                                        {% elif box.name == "location"%}
                                            <label>Location:</label>
                                        {% elif box.name == "patient"%}
                                            <label>Patient:</label>
                                        {% elif box.name == "doctor"%}
                                            <label>Doctor:</label>
                                        {% endif %}
                                        {{ box }}
                                    </div>
                                    {% endfor %}
                                    <input style="margin-left:10px;float:right;" class="btn btn-primary btn-md" type="submit" value="Create">
                                    <button style="float:right;" type="button" class="btn btn-default btn-md" data-dismiss="modal">Cancel</button>
                                  </form>
                                </div>
                                <div class="modal-footer">
                                </div>
                              </div>
                            </div>
                          </div>
                          <!-- Update Appointment Modal -->
                          <div class="modal fade" id="updateEventModal" role="dialog">
                            <div class="modal-dialog">

                              <!-- Modal content-->
                              <div class="modal-content">
                                <div class="modal-header">
                                  <button type="button" class="close" data-dismiss="modal">&times;</button>
                                  <h4 id="modalUpdateTitle" class="modal-title"></h4>
                                </div>
                                <div id="modalUpdateBody" class="modal-body">
                                     <form role="form" method="post" action=".">{% csrf_token %}
                                        <div class="form-group">
                                            <label>Title:</label>
                                            <input class="form-control" id="update_title" maxlength="20" type="text">
                                        </div>
                                        <br>
                                        <div class="form-group">
                                            <label>Description:</label>
                                            <input class="form-control" id="update_description" maxlength="140" size="50" type="text">
                                        </div>
                                        <br>
                                        <div class="form-group">
                                            <label>Date:</label>
                                            <input class="form-control" id="update_date" maxlength="8" type="text">
                                        </div>
                                        <br>
                                        <div class="form-group">
                                            <label>Start:</label>
                                            <input class="form-control" id="update_start" maxlength="5" type="text">
                                        </div>
                                        <br>
                                        <div class="form-group">
                                            <label>End:</label>
                                            <input class="form-control" id="update_end" maxlength="5" type="text">
                                        </div>
                                    </form>
                                    <button style="margin-left:10px;float:right;" type="submit" class="btn btn-primary" id="save" data-dismiss="modal">Save</button>
                                    <button style="float:right;" type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                                </div>
                                <div class="modal-footer">
                                </div>
                              </div>
                            </div>
                          </div>
                          <!-- THE CALENDAR -->
                        <div id="calendar" class="fc fc-ltr fc-unthemed"></div>
                        </div><!-- /.col -->
                      </div><!-- /.row -->
                    </div><!-- ./box-body -->
                  </div><!-- /.box -->
                </div><!-- /.col -->

{#================================================================================================================================#}
{#==========================                                                       ===============================================#}
{#==========================                    List of Patients                   ===============================================#}
{#==========================                                                       ===============================================#}
{#================================================================================================================================#}

                <div id="container">
                <div class="col-md-3">
                  <div class="box" style="margin-left: 30%;">
                    <div class="box-header with-border">
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-12">
                                        <div class="table-responsive">
                                            <table class="table table-bordered table-hover table-striped">
                                                <thead> <h2>List Of Patients</h2> </thead>
                                                <tbody>
                                                {% if not item.listOfPatients.all %}
                                                    <tr>
                                                    <td>You Currently Have No Patients</td>
                                                    </tr>
                                                {% endif %}
                                                {% for patient in item.listOfPatients.all %}
                                                <tr>
                                                    <td><a href="/patients-doctor/{{ patient.id }}/{{ item.id }}/">{{ patient }}</a></td>
                                                </tr>
                                                {% endfor %}
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                      </div>
                    </div>
                </div>

            <div class="col-lg-10">
            <div id="container" style="width: 75%;">
                <div class="col-lg-10">
                  <div class="box">
                    <div class="box-header with-border">
                        <div class="box-body">
                            <div class="row">
                                <div class="col-lg-1" style="width: 100%;">
                                    <div class="table-responsive"  style="width: 100%;">
                                        <table class="table table-bordered table-hover table-striped">
                                            <thead> <h2>Alerts</h2> </thead>
                                            <tbody >
                                            {% for message in latest_inbox_list %}
                                            <tr>
                                                <td>Sender: </td>
                                                <td>{{ message.sender }}</td>
                                                <td>message: </td>
                                                <td>{{ message.message_text }}</td>
                                            </tr>

                                            {% endfor %}
                                            </tbody>
                                        </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                      </div>
                    </div>
                </div>
                </div>

              </div><!-- /.row -->
              <!-- Main row -->

        </section>
          <!-- /.content -->
      </div><!-- /.content-wrapper -->
      <footer class="main-footer">
        <div class="pull-right hidden-xs">
          <b>Version</b> 2.3.0
        </div>
        <strong>Copyright &copy; 2014-2015 <a href="http://almsaeedstudio.com">Almsaeed Studio</a>.</strong> All rights reserved.
      </footer>



    <!-- jQuery 2.1.4 -->
    <script src="{% static 'main_site/css/plugins/jQuery/jQuery-2.1.4.min.js' %}"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
      $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.5 -->
    <script src="{% static 'main_site/js/bootstrap.min.js' %}"></script>
    <!-- Morris.js charts -->
    <script src="{% static 'main_site/js/raphael-min.js' %}"></script>
    <script src="{% static 'main_site/css/plugins/morris/morris.min.js' %}"></script>
    <!-- Sparkline -->
    <script src="{% static 'main_site/css/plugins/sparkline/jquery.sparkline.min.js' %}"></script>
    <!-- jvectormap -->
    <script src="{% static 'main_site/css/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js' %}"></script>
    <script src="{% static 'main_site/css/plugins/jvectormap/jquery-jvectormap-world-mill-en.js' %}"></script>
    <!-- jQuery Knob Chart -->
    <script src="{% static 'main_site/css/plugins/knob/jquery.knob.js' %}"></script>
    <!-- Slimscroll -->
    <script src="{% static 'main_site/css/plugins/slimScroll/jquery.slimscroll.min.js' %}"></script>
    <!-- FastClick -->
    <script src="{% static 'main_site/css/plugins/fastclick/fastclick.min.js' %}"></script>
    <!-- AdminLTE App -->
    <script src="{% static 'main_site/js/app.min.js' %}"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="{% static 'main_site/js/demo.js' %}"></script>
    <!-- fullCalendar -->
    <script src="{% static 'main_site/js/fullcalendar/lib/moment.min.js'%}"></script>
    <script src="{% static 'main_site/js/fullcalendar/fullcalendar.js'%}"></script>
    <script>

            $(document).ready(function() {
                // page is now ready, initialize the calendar...
                $('#id_title').attr("class","form-control");
                $('#id_description').attr("class","form-control");
                $("#id_date").attr("placeholder","MM/DD/YY").attr("class","form-control");
                $("#id_start_time").attr("placeholder","Enter military time i.e. 06:00").attr("class","form-control");
                $("#id_end_time").attr("placeholder","Enter military time i.e. 14:00").attr("class","form-control");
                $("#id_location").attr("class","form-control");
                $("#id_patient").attr("class","form-control");
                $("#id_doctor").attr("class","form-control");

                //locates text field from update modal
                var titleVal = $("#update_title");
                var descriptionVal = $("#update_description");
                var dateVal = $("#update_date");
                var startVal = $("#update_start");
                var endVal = $("#update_end");

                //hide apt_id in modal
                $("#modalID").hide();

                $('#calendar').fullCalendar({
                    customButtons: {
                        myCustomButton: {
                            text: '+',
                            click: function(start, end, date) {
                                $("#createEventModal").modal();
                            }
                        }
                    },
                    header:{
                        left:"today",
                        center:"prev title next",
                        right:"myCustomButton"
                    },
                    editable: true,
                    selectable: true,
                    defaultView:"agendaDay",
                    events:{{ eventsList|safe }},
                    eventClick: function(event, jsEvent, view){
                        var modalBodyText = event.patient + "\n" + event.description;
                        $('#modalTitle').html(event.title);
                        $('#modalPatient').html(event.patient);
                        $('#modalID').html(event.id);
                        $('#modalDescription').html(event.description);
                        $('#modalLocation').html(event.location);
                        $('#modalDate').html(event.date);
                        $('#displayEventModal').modal();
                    },
                    select: function(start, end, date) {
                        var timeDate = parseTimeDate(start,end);
                        $("#id_date").val(timeDate["date"]);
                        $("#id_start_time").val(timeDate["start"]);
                        $("#id_end_time").val(timeDate["end"]);
                        $("#createEventModal").modal();
                    },
                    timeFormat: 'h(:mm)t'

                });
                $("#update").click(function(){
                    //show update modal
                    $('#updateEventModal').modal();

                    //get appointmentID
                    var apt_id = $("#modalID").text();

                    $.ajaxSetup({
                        beforeSend: function(xhr, settings) {
                            if (settings.type == 'POST' || settings.type == 'PUT' || settings.type == 'DELETE'|| settings.type == 'GET') {
                                function getCookie(name) {
                                    var cookieValue = null;
                                    if (document.cookie && document.cookie != '') {
                                        var cookies = document.cookie.split(';');
                                        for (var i = 0; i < cookies.length; i++) {
                                            var cookie = jQuery.trim(cookies[i]);
                                            // Does this cookie string begin with the name we want?
                                            if (cookie.substring(0, name.length + 1) == (name + '=')) {
                                                cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                                                break;
                                            }
                                        }
                                    }
                                    return cookieValue;
                                }
                                if (!(/^http:.*/.test(settings.url) || /^https:.*/.test(settings.url))) {
                                    // Only send the token to relative URLs i.e. locally.
                                    xhr.setRequestHeader("X-CSRFToken", getCookie('csrftoken'));
                                }
                            }
                        }
                    });
                    $.ajax({
                        url:"/update-appointment/"+apt_id+"/",
                        type:"GET",
                        data:{},
                        success:function(json){
                            $("#update_title").val(json['title']);
                            descriptionVal.val(json['description']);
                            dateVal.val(json['date']);
                            startVal.val(json['start']);
                            endVal.val(json['end']);
                        }
                    });
                });
                $("#save").click(function(){
                    //get appointmentID
                    var apt_id = $("#modalID").text();

                    $.ajaxSetup({
                        beforeSend: function(xhr, settings) {
                            if (settings.type == 'POST' || settings.type == 'PUT' || settings.type == 'DELETE'|| settings.type == 'GET') {
                                function getCookie(name) {
                                    var cookieValue = null;
                                    if (document.cookie && document.cookie != '') {
                                        var cookies = document.cookie.split(';');
                                        for (var i = 0; i < cookies.length; i++) {
                                            var cookie = jQuery.trim(cookies[i]);
                                            // Does this cookie string begin with the name we want?
                                            if (cookie.substring(0, name.length + 1) == (name + '=')) {
                                                cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                                                break;
                                            }
                                        }
                                    }
                                    return cookieValue;
                                }
                                if (!(/^http:.*/.test(settings.url) || /^https:.*/.test(settings.url))) {
                                    // Only send the token to relative URLs i.e. locally.
                                    xhr.setRequestHeader("X-CSRFToken", getCookie('csrftoken'));
                                }
                            }
                        }
                    });
                    $.ajax({
                        url:"/update-appointment/"+apt_id+"/",
                        type:"POST",
                        data:{title:titleVal.val(), description:descriptionVal.val(), date:dateVal.val(), start:startVal.val(), end:endVal.val()},
                        success:function(){
                            location.reload();
                        }
                    });
                });
                $("#delete").click(function(){
                    //get appointmentID
                    var apt_id = $("#modalID").text();

                    $.ajaxSetup({
                        beforeSend: function(xhr, settings) {
                            if (settings.type == 'POST' || settings.type == 'PUT' || settings.type == 'DELETE'|| settings.type == 'GET') {
                                function getCookie(name) {
                                    var cookieValue = null;
                                    if (document.cookie && document.cookie != '') {
                                        var cookies = document.cookie.split(';');
                                        for (var i = 0; i < cookies.length; i++) {
                                            var cookie = jQuery.trim(cookies[i]);
                                            // Does this cookie string begin with the name we want?
                                            if (cookie.substring(0, name.length + 1) == (name + '=')) {
                                                cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                                                break;
                                            }
                                        }
                                    }
                                    return cookieValue;
                                }
                                if (!(/^http:.*/.test(settings.url) || /^https:.*/.test(settings.url))) {
                                    // Only send the token to relative URLs i.e. locally.
                                    xhr.setRequestHeader("X-CSRFToken", getCookie('csrftoken'));
                                }
                            }
                        }
                    });
                    $.ajax({
                        url:"/cancel-appointment/"+apt_id+"/",
                        type:"DELETE",
                        data:{},
                        success:function(){
                            location.reload();
                        }
                    });
                });

            });
            function parseTimeDate(start, end){
                var starttime = moment(start).toString();
                var endtime = moment(end).toString();

                var tempStartArr = starttime.split(":");
                var tempEndArr = endtime.split(":");

                var date = parseDate(start);

                var finalStart = tempStartArr[0].substring(tempStartArr[0].length-2,tempStartArr[0].length)+":"+tempStartArr[1];
                var finalEnd = tempEndArr[0].substring(tempEndArr[0].length-2,tempEndArr[0].length)+":"+tempEndArr[1];

                var timeDate = {
                    date:date,
                    start:finalStart,
