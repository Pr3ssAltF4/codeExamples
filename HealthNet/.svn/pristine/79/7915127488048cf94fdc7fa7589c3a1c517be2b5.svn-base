            $(document).ready(function() {
                // page is now ready, initialize the calendar...

                //href for appointment. Will need when pulling up calendar info
                //<li><a class="apt" href="/appointmentDoctor/{{ items.id }}/{{id}}/">{{ items.title }}{{ items.date }}T{{ items.start_time }}</a></li>

                //hide list items
                $(".Capt").hide();

                $('#calendar').fullCalendar({

                    header:{
                        left:
                            "prev, next today",
                        center:"title",
                        right:"month, agendaWeek, agendaDay"
                    },
                    defaultView:"agendaDay",
                    events: function(start, end, timezone, callback) {
                        //array to store events
                        var events = [];

                        //get appointment node
                        var appointmentList = $(".apt");

                        //parseTime("2Am");

                        appointmentList.each(function(index){
                            events.push(
                                {
                                    title:$(this).find("#title").text(),
                                    start:parseDate($(this).find("#date").text()) + $(this).find("#time").text(),
                                    editable:"false"
                                }
                            );
                        });

                        //alert(events[0]["start"]);
                        callback(events);

                    },
                    timeFormat: 'h(:mm)t'

                });
            });
        </script>
        </div>
  </body>
