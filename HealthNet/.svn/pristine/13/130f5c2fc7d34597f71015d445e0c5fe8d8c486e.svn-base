{% block title %}
<title>Edit Info|HealthNet</title>
{% endblock %}
{% block content %}
<div>
    <label>Update Info</label>
    <form method="post" action=".">{% csrf_token %}
        {% for box in form %}
        <br>
        {{box.name}} : {{ box }}
        <br>
        {% endfor %}
        <br>
        <input type="submit" value="update">
    </form>
</div>
<style>
    form {
    font-style: italic;
    font-size: 15px;
    }
</style>
{% endblock %}